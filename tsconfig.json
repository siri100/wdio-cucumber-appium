{
  "compilerOptions": {
      "sourceMap": false,
      "target": "es2020",
      "module": "NodeNext",
      "moduleResolution": "Node16",
      "removeComments": true,
      "noImplicitAny": true,
      "strictPropertyInitialization": true,
      "strictNullChecks": true,
      "types": [
        "node", "@wdio/globals/types","@wdio/cucumber-framework", "expect-webdriverio",
      ],
  },
  "include": [
      "./config/**/*.ts",
      "./tests/**/*.ts"
, "config/wdio.android.conf.mts"  ]
}